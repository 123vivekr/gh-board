{
  "parser": "babel-eslint",
  "plugins": [ "react" ],
  "env": {
    "es6": true,
    "browser": true,
  },
  "ecmaFeatures": {
    "templateStrings": true,
    "arrowFunctions": true,
    "modules": true,
    "blockBindings": true,
    "forOf": true,
    "jsx": true
  },
  "rules": {
    "react/jsx-uses-vars": 1,
    "new-cap": 0,
    "no-console": 1,
    "semi": 1
  }
}
